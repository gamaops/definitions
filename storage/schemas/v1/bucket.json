{
	"$id": "storage.v1.Bucket",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"minLength": 2,
			"maxLength": 120
		},
		"acceptedMimeTypes": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"uniqueItems": true
		},
		"tags": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"uniqueItems": true
		},
		"maxSize": {
			"type": "integer",
			"minimum": 0
		},
		"processor": {
			"type": "integer",
			"enum": [
				0
			]
		},
		"upload": {
			"type": "object",
			"properties": {
				"url": {
					"type": "string",
					"format": "uri"
				},
				"fieldName": {
					"type": "string",
					"minLength": 1,
					"maxLength": 40
				},
				"maxCount": {
					"type": "integer",
					"minimum": 0
				},
				"tokenExpirationSeconds": {
					"type": "integer",
					"minimum": 0
				}
			},
			"required": [
				"url",
				"fieldName",
				"maxCount",
				"tokenExpirationSeconds"
			]
		}
	},
	"required": [
		"name",
		"upload",
		"acceptedMimeTypes",
		"maxSize",
		"tags",
		"processor"
	]
}